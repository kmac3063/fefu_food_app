<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fefu_food_app">

    <application
        android:allowBackup="true"
        android:icon="@drawable/layout_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/layout_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".OrderActivity"></activity>
        <activity android:name=".BoardActivity" />
        <activity android:name=".BasketActivity" />
        <activity android:name=".CustomerMenuActivity" />
        <activity android:name=".ProductsActivity" />
        <activity android:name=".MenuActivity" />
        <activity android:name=".CategoriesActivity" />
        <activity android:name=".ProfileActivity" />
        <activity
            android:name=".SplashScreenActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>